alias arca='arc -qx -- .'
alias xclipc='xclip -selection clipboard'
alias gitaa='git add --all'
alias gitxds='git diff --staged'
alias gitxa='gitaa && alias gitxc'
alias gitxd='gitxds | xclipc && gitxds'
alias ap='pkgmgr path alias'
alias au='(cd $(ap) && make update) && source ~/.config/shell/aliases'
alias hw='echo hello world'
